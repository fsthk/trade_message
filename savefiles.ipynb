   "outputs": [],
   "source": [
    "#  csv形式で保存\n",
    "def saveCSV(data):\n",
    "    with open(\"data.csv\", \"w\", newline=\"\") as f:\n",
    "        csv_writer = csv.writer(f)\n",
    "        csv_writer.writerows(data)"
